{
  "name": "sayn-debugger",
  "main": "./lib/sayn-debugger",
  "version": "0.2.0",
  "description": "Develop and Debug SAYN projects",
  "keywords": [
    "sayn",
    "develop sayn",
    "debug sayn",
    "pdb"
  ],
  "activationCommands": {
    "atom-workspace": [
      "sayn-debugger:toggle",
      "sayn-debugger:breakpoint"
    ]
  },
  "repository": "https://github.com/dpo/atom-python-debugger",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0"
  },
  "activationHooks": [
    "language-python:grammar-used"
  ],
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
